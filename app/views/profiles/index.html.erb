<h1>All My Profiles</h1>
<h2><%= current_user.profiles.name %></h2>
<h2><%= current_user.id %></h2>
<h2><a href="/profiles/new">(New Profile)</a></h2>
<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |post| %>
      <tr>
        <td><%= post.body %></td>
        <td><%= post.title %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
