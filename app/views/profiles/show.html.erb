<h1><%= @profile.name%></h1>
<div class:'location'>
  <p>City: <%= @profile.city %></p>
  <p>State: <%= @profile.state %></p>
</div>

<div class:'info'>
  <p>Breed: <%= @profile.breed %></p>
  <p>Age: <%= @profile.age %></p>
  <p>Gender: <%= @profile.gender %></p>
  <img><%= image_tag @profile.pro_pic %></img>
  <p>Description <%= @profile.description %>
</div>
<p>  <%= link_to 'User Profile', profiles_path %></p>
