<h1>New Profile</h1>
<%= form_for @profile do |f| %>


  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :gender %>
  <%= f.check_box :gender %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :city %>
  <%= f.text_field :city %>

 <%= select_tag 'State', options_for_select(us_states),:name=>"profile[state]",:id=>"state"%>


  <%= f.label :age %>
  <%= f.number_field :age %>

  <%=f.label :pro_pic %>
  <%= f.file_field :pro_pic, class: "form-control" %>

  <%= f.label :description %>
  <%= f.text_field :description, class: "description_para" %>

  <p>
    <%= f.submit %>
    <%= link_to 'Back', :back, class: "back" %>
  </p>
<% end %>
